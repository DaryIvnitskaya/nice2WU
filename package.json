{
  "name": "someName",
  "version": "0.0.0",
  "description": "Тестовый проект",
  "main": "index.html",
  "scripts": {
    "watch:sass": "node-sass -rw sass/main.scss css/style.css",
    "devprefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.css -o css/style.css",
    "prefixwatch": "npm-run-all watch:sass devprefix:css",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver prefixwatch",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "",
  "license": "ISC",
  "homepage": "/",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "grunt": "*",
    "grunt-autoprefixer": "*",
    "grunt-contrib-imagemin": "^3.1.0",
    "grunt-contrib-less": "*",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-watch": "*",
    "grunt-svgstore": "^2.0.0",
    "http-server": "*",
    "less": "*",
    "node-sass": "^4.5.3",
    "node-static": "*",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {
    "sass": "^1.17.0"
  }
}
